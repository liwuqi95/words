  <%= javascript_include_tag 'application' %>


  <%= render 'layouts/head' %>
</head>


<body class="adminbody">


<div id="main">

  <%= render 'layouts/header' %>

  <%= render 'layouts/sidebar' %>

  <div class="content-page">

